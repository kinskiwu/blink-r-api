  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/startServer.ts --exec ts-node src/startServer.ts",
    "start": "NODE_ENV=production node ./dist/startServer.js",
    "build": "rimraf ./dist && tsc && tsc-alias",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --fix --ext .ts",
    "format:all": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts)\"",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts)\"",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "redis": "^4.6.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.11.20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.6",
    "mongoose": "^8.2.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  }
}
